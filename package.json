{
  "name": "epilot",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:debug": "next build --debug-prerender",
    "prebuild": "pnpm run type-check & pnpm run codegen",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "format": "prettier --write .",
    "codegen": "graphql-codegen --config codegen.ts",
    "amplify:push": "amplify push && pnpm codegen",
    "amplify:compile": "amplify api gql-compile && pnpm codegen"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.974.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-query": "^5.90.20",
    "@vercel/analytics": "^1.6.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql": "^16.12.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.4",
    "next-auth": "5.0.0-beta.30",
    "next-intl": "^4.3.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sass": "^1.97.3",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^6.1.1",
    "@graphql-codegen/client-preset": "^5.2.2",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tailwindcss/postcss": "latest",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.5.13",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
